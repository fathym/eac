{
  "name": "@fathym/cli",
  "version": "0.0.4",
  "description": "The CLI used for code first flows with Fathym EaC.",
  "author": "Fathym, Inc",
  "bin": {
    "fathym": "./bin/run"
  },
  "homepage": "https://github.com/fathym/eac",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "fathym/eac",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@fathym/common": "^0.0.24",
    "@oclif/core": "^1.22.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.9",
    "@semanticjs/common": "^0.64.0",
    "execa": "^6.1.0",
    "inquirer": "^9.1.4",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.16",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.3",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.10",
    "@types/supports-color": "^8.1.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "fathym",
    "dirname": "fathym",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command-incomplete.js"
    },
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Commands used for managing user authentication with Fathym."
      },
      "enterprises": {
        "description": "Commands used for interacting with the users available enterprises."
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run build && npm publish --access public",
    "fathym": "npm run build && \"./bin/run.cmd\"",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Fathym, Inc/eac/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
