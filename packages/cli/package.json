{
  "name": "@fathym/cli",
  "version": "0.0.171",
  "description": "The CLI used for code first flows with Fathym EaC.",
  "author": "Fathym, Inc",
  "bin": {
    "fathym": "./bin/run",
    "fym": "./bin/run",
    "ftm": "./bin/run"
  },
  "homepage": "https://github.com/fathym/eac",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "fathym/eac",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@microsoft/signalr": "^7.0.2",
    "@oclif/core": "^2.1.1",
    "@oclif/plugin-help": "^5.2.4",
    "@oclif/plugin-not-found": "^2.3.18",
    "@oclif/plugin-plugins": "^2.3.1",
    "@oclif/plugin-warn-if-update-available": "^2.0.26",
    "@semanticjs/common": "^0.69.0",
    "axios": "^1.3.2",
    "enquirer": "^2.3.6",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.0",
    "handlebars": "^4.7.7",
    "immer": "^9.0.19",
    "listr2": "^5.0.7",
    "open": "^8.4.1",
    "path": "^0.12.7",
    "simple-oauth2": "^5.0.0",
    "table-layout": "^3.0.0",
    "tar": "^6.1.13",
    "tar-fs": "^2.1.1",
    "user": "^0.0.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.5",
    "@types/chai": "^4",
    "@types/express": "^4.17.15",
    "@types/fs-extra": "^9.0.13",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "@types/simple-oauth2": "^4.1.1",
    "@types/supports-color": "^8.1.1",
    "@types/tar-fs": "^2.0.1",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3.6.3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "fathym",
    "dirname": "fathym",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-warn-if-update-available"
    ],
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command-incomplete.js"
    },
    "topicSeparator": " ",
    "topics": {
      "auth": {
        "description": "Commands used for managing user authentication with Fathym."
      },
      "enterprises": {
        "description": "Commands used for interacting with the users available enterprises."
      }
    },
    "warn-if-update-available": {
      "timeoutInDays": 1,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.  Run `fathym upgrade`."
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "deploy": "fathym git && npm version patch && npm run deploy:all && npm run deploy:refresh",
    "deploy:all": "npm publish --access public",
    "deploy:refresh": "node -e \"setTimeout(()=>{},500)\" && npm run deploy:commit && fathym upgrade",
    "deploy:commit": "fathym git \"Deploy complete, syncing versions and readme updates\"",
    "ftm": "node -e \"setTimeout(()=>{},0)\" && \"./bin/run.cmd\"",
    "fathym": "npm run build && \"./bin/run.cmd\"",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "thinky": "npm run build && \"./bin/run.cmd\"",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Fathym, Inc/eac/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
